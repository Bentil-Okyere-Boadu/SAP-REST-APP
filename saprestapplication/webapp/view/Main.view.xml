<mvc:View controllerName="saprestapplication.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <Table id="table1" items="{path: 'DataModel>/'}" class="sapUiSizeCompact">
                <columns>
                    <Column id="col1">
                        <Label id="_IDGenLabel1" text="Title"/>
                    </Column>
                    <Column id="col2">
                        <Label id="_IDGenLabel2" text="Description"/>
                    </Column>
                    <Column id="_IDGenColumn1">
                        <Label id="_IDGenLabel3" text="Total Stars"/>
                    </Column>
                    <Column id="_IDGenColumn2">
                        <Label id="_IDGenLabel4" text="Star Number"/>
                    </Column>
                    <Column id="_IDGenColumn3">
                        <Label id="_IDGenLabel5" text="Category"/>
                    </Column>
                    <Column id="_IDGenColumn4">
                        <Label id="_IDGenLabel6" text="Price"/>
                    </Column>
                    <Column id="_IDGenColumn5">
                        <Label id="_IDGenLabel7" text="Delivery Time"/>
                    </Column>
                    </columns>
                    <ColumnListItem id="_IDGenColumnListItem1">
                        <Text id="_IDGenText1" text="{DataModel>title}"/>
                        <Text id="_IDGenText2" text="{DataModel>desc}"/>
                        <Text id="_IDGenText3" text="{DataModel>totalStars}"/>
                        <Text id="_IDGenText4" text="{DataModel>starNumber}"/>
                        <Text id="_IDGenText5" text="{DataModel>cat}"/>
                        <Text id="_IDGenText6" text="{DataModel>price}"/>
                        <Text id="_IDGenText7" text="{DataModel>deliveryTime}"/>
                    </ColumnListItem>
            </Table>
        </content>
    </Page>
</mvc:View>
